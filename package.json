    "mocha",
    "chai",
    "node",
    "puppeteer",
    "css regression"
  ],
  "scripts": {
    "eslint": "./node_modules/.bin/eslint ./app/**/*.js",
    "mocha": "./node_modules/mocha/bin/mocha",
