{
  "name": "chigai-core",
  "version": "1.4.2",
  "description": "Chigai: modern visual regression testing. CORE module.",
  "main": "./app/main.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.5.5"
  },
  "repository": {
    "type": "git",
