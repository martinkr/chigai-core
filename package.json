{
  "name": "chigai-core",
  "version": "1.0.0",
  "description": "Chigai: modern visual regression testing. CORE module.",
  "main": "./app/main.js",
  "author": "Martin Krause",
  "license": "MIT",
  "engines" : { "node" : ">=8.5.5" },
  "keywords": [
    "visual",
    "regression",
    "testing",
    "ci",
    "continious integration",
    "bdd",
    "tdd",
    "mocha",
    "chai",
    "node",
    "puppeteer",
    "css regression"
  ],
  "scripts": {
    "start": "node ./app/main.js",
    "mocha": "./node_modules/mocha/bin/mocha",
    "nyc": "./node_modules/.bin/nyc --clean ./node_modules/.bin/mocha",
    "coverage": "./node_modules/.bin/nyc check-coverage --lines 100 --functions 100 --branches 100 --statements 100",
    "report": "./node_modules/.bin/nyc report --reporter=lcov --reporter=html",
    "test": "yarn nyc && yarn report && yarn coverage",
    "testLegacy": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha && ./node_modules/.bin/istanbul check-coverage --statement 100 --branch 100 --function 100"
  },
  "dependencies": {
    "blink-diff": "^1.0.13",
    "fs-extra-plus": "^0.1.3",
    "puppeteer": "^0.11.0"
  },
  "devDependencies": {
    "proxyquire": "^1.8.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "chai": "4.1.1",
    "chai-as-promised": "^7.1.1",
    "eslint": "4.4.1",
    "eslint-loader": "1.9.0",
    "eslint-watch": "3.1.2",
    "istanbul": "0.4.5",
    "jsdoc": "^3.5.4",
    "loke-jsdoc-theme": "^2.1.0",
    "mocha": "3.5.0",
    "mocha-loader": "1.1.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.13.0",
    "webpack": "^3.5.4"
  }
}
